generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id() @default(autoincrement())
  chat_id String @unique
  fullName String
  age String?
  tel_1 String
  tel_2 String
  username String
  addres_doyimiy String
  addres_hozir String
  universitet String
  yonalish String
  daraja String
  portfoly_link String
  rezumey_link String
  ish_holati String
  till String[]
}